<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

@if(TempData["exitoso"] != null)
{
    <script>
        Swal.fire({
          icon: "success",
          title: '@TempData["exitoso"]',
               
        });
    </script>
}

@if (TempData["error"] != null)
{
    <script>
        Swal.fire({
            icon: "error",
            title: '@TempData["error"]',
            
        });
    </script>
}


